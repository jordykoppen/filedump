{
  "name": "filedump-monorepo",
  "version": "1.0.1",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "bun --filter @filedump/server dev",
    "build": "bun --filter @filedump/server build",
    "build:executable": "bun --filter @filedump/server build:executable",
    "start": "bun --filter @filedump/server start"
  },
  "devDependencies": {
    "@types/bun": "latest"
  }
}
